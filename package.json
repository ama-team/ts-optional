{
  "name": "@ama-team/optional",
  "version": "0.1.2",
  "main": "dist/index.js",
  "repository": "git@github.com:ama-team/ts-optional.git",
  "author": "AMA Team <dev@amagroup.ru>",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc -d",
    "test": "nyc mocha",
    "report": "allure generate -o report/allure -- metadata/allure",
    "coverage:publish": "cat report/coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/sinon": "^4.3.0",
    "allure-commandline": "^2.5.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^5.0.4",
    "mocha-allure-reporter": "^1.3.2",
    "mocha-junit-reporter": "^1.17.0",
    "mocha-multi-reporters": "^1.1.7",
    "mocha-typescript": "^1.1.12",
    "nyc": "^11.6.0",
    "sinon": "^4.4.6",
    "source-map-support": "^0.5.4",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": "lcov",
    "report-dir": "report/coverage",
    "temp-directory": "metadata/nyc",
    "all": true
  }
}
